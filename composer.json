{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "amendozaaguiar/filament-route-statistics": "^3.0",
        "awcodes/filament-badgeable-column": "^2.3",
        "awcodes/filament-tiptap-editor": "^3.4",
        "bezhansalleh/filament-exceptions": "^2.1",
        "croustibat/filament-jobs-monitor": "^2.5",
        "daylaborers/laravel-mjml": "^1.0",
        "eightynine/filament-advanced-widgets": "^3.0",
        "evanschleret/lara-mjml": "^0.1.2",
        "filament/filament": "^3.2",
        "filament/forms": "^3.2",
        "filament/tables": "^3.2",
        "firebase/php-jwt": "^6.10",
        "flowframe/laravel-trend": "*",
        "hydrat/filament-table-layout-toggle": "^2.0",
        "lara-zeus/accordion": "^1.1",
        "laravel/framework": "^11.0",
        "laravel/nightwatch": "^1.7",
        "laravel/tinker": "^2.9",
        "league/html-to-markdown": "^5.1",
        "livewire/livewire": "^3.5",
        "openspout/openspout": "4.28",
        "saade/filament-fullcalendar": "^3.2",
        "saade/filament-laravel-log": "^3.2",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-permission": "^6.7",
        "staudenmeir/belongs-to-through": "^2.5",
        "subfission/cas": "^5.0",
        "z3d0x/filament-logger": "^0.7.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel-lang/lang": "^15.5",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
